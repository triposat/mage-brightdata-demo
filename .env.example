# ===========================================
# Bright Data Credentials
# ===========================================

# Web Scraper API Token (required for Amazon pipeline)
# Get from: https://brightdata.com/cp/api_tokens
BRIGHT_DATA_API_TOKEN=your_api_token_here

# Customer ID (required for Web Unlocker pipeline)
# Format: brd-customer-hl_xxxxx
BRIGHT_DATA_CUSTOMER_ID=brd-customer-hl_xxxxx

# Web Unlocker Zone Password (required for Web Unlocker pipeline)
# Get from: https://brightdata.com/cp/zones
BRIGHT_DATA_ZONE_PASSWORD=your_zone_password_here

# Legacy proxy settings (optional)
BRIGHT_DATA_ZONE=web_unlocker1
BRIGHT_DATA_PASSWORD=
BRIGHT_DATA_PROXY_HOST=brd.superproxy.io
BRIGHT_DATA_PROXY_PORT=33335

# ===========================================
# Notifications (optional)
# ===========================================

# Slack Incoming Webhook URL
# Create at: https://api.slack.com/messaging/webhooks
SLACK_WEBHOOK_URL=

# Generic webhook URL for alerts
ALERT_WEBHOOK_URL=

# ===========================================
# PostgreSQL Database
# ===========================================

POSTGRES_USER=mage
POSTGRES_PASSWORD=mage_password
POSTGRES_DB=scraped_data
